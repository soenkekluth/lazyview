{
  "presets": [
    ["es2015", { "loose": true }],
    "stage-0"
  ],
  "env": {
    "dist": {
      "plugins": [
        "add-module-exports"
      ]
    },
    "lib": {
      "presets": [
        "es2015-tree-shaking"
      ],
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }],
        "add-module-exports"
      ]
    },
    "es": {
      "presets": [
        "es2015-tree-shaking"
      ]
    }
  }
}
